{
  "name": "A browser action that only shows images from the page with ability to zoom on click",
  "description": "Only images with zoom",
  "version": "2.0",
  "permissions": ["activeTab"],
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action": {
      "default_title": "Only show Images",
      "default_icon": "icon.png"
  },
  "manifest_version": 2
}
